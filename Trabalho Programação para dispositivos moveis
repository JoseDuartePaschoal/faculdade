import React, {Component} from 'react';
import {View, Text, Image} from 'react-native';

class App extends Component{
	render(){
	 return(
	  <View>
	    <Text>Olá Mundo!!!</Text>
	    <Text>Meu primeiro App"</Text>
	    <Text style={{color:'#FF000',fontSize: 25, margin: 15}}> Teste de Propridade</Text>
	    <Image
	      source={{ uri: 'https://i2.wp.com/telhasparana.com.br/wp-content/uploads/2021/07/supernova-chumbo.png?fit=750%2C484&ssl=1' }}
	      style={{ width: 300, height: 300 }}
	    />
	  </View>

	);
  }
}
export default App;






COPIADO
import React, {Component} from 'react';
import {View,Text,Image} from 'react-native';
class App extends Component{
render(){
return(
<View>
<Text>Olá Mundo!!!</Text>
<Text>Meu Primeiro App!</Text>
<Text style={{color:'#FF0000',fontSize: 25, margin:15}}>Teste de Propriedade</Text>
<Image
source={{ uri: 'https://reactnative.dev/img/tiny_logo.png' }}
style={{ width: 300, height: 300 }}
/>
</View>
);
}
}
export default App;




